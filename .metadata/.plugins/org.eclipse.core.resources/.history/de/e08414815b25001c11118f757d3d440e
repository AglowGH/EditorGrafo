
import java.awt.geom.*;

public class Nodo 
{
	private Point2D coordenadas;
	private Nodo next = null;
	
	public Nodo(Point2D c)
	{
		coordenadas = c;
	}
	
	public void setCoordenadas(Point2D c)
	{
		coordenadas = c;
	}
	
	public Point2D getCoordenadas()
	{
		return coordenadas;
	}
	
	public void setNext(Nodo c)
	{
		next = c;
	}
	
	public Nodo getNext()
	{
		return next;
	}
}
